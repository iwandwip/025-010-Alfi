{
  "expo": {
    "name": "Alfi App",
    "slug": "alfi-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-money.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icon-money.png",
      "resizeMode": "contain",
      "backgroundColor": "#F50057"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-money.png",
        "backgroundColor": "#F50057"
      },
      "package": "com.alfi.alfiapp"
    },
    "web": {
      "favicon": "./assets/icon-money.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "firebase-auth-template",
    "extra": {
      "router": {}
    }
  }
}
